<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx4="http://www.rbsas.co/schemas/RBInformationServices/Schemas/Version 1.0.0/InputOutput/MerchantSystemTransaction.xsd" xmlns:pfx3="http://www.rbsas.co/schemas/RBInformationServices/Schemas/Version 1.0.0/Request/MerchantSystemTransactionRequest.xsd" xmlns:pfx2="http://www.rbsas.co/schemas/RBDAL/Schemas/Version 1.0.0/BusinessObjects/Error.xsd" xmlns:pfx="http://www.rbsas.co/schemas/RBInformationServices/Schemas/Version 1.0/Request/SetCardRequest.xsd">
    <xsd:import namespace="http://www.rbsas.co/schemas/RBInformationServices/Schemas/Version 1.0/Request/SetCardRequest.xsd" schemaLocation="/Schemas/Version 1.0.0/Request/SetCardRequest.xsd"/>
    <xsd:import namespace="http://www.rbsas.co/schemas/RBDAL/Schemas/Version 1.0.0/BusinessObjects/Error.xsd" schemaLocation="/GeneralSchemas/Version 1.0.0/BusinessObjects/Error.xsd"/>
    <xsd:import namespace="http://www.rbsas.co/schemas/RBInformationServices/Schemas/Version 1.0.0/InputOutput/MerchantSystemTransaction.xsd" schemaLocation="/MerchanSystem.core/Schemas/MerchantSystemTransaction.xsd"/>
    <xsd:import namespace="http://www.rbsas.co/schemas/RBInformationServices/Schemas/Version 1.0.0/Request/MerchantSystemTransactionRequest.xsd" schemaLocation="/MerchanSystem.core/Schemas/MerchantSystemTransactionRequest.xsd"/>
    <pd:name>MerchanSystem.core/Implementation/ServiceProcess/SetMerchantSystemTransactionSelector.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx3:MerchantSystemTransactionRequest"/>
    <pd:startX>43</pd:startX>
    <pd:startY>140</pd:startY>
    <pd:returnBindings>
        <ResponseString>
            <xsl:value-of select="$varResponseString/ResponseString"/>
        </ResponseString>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="ResponseString" type="xsd:string"/>
    </pd:endType>
    <pd:endX>744</pd:endX>
    <pd:endY>145</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <varResponseString>
            <xsd:element name="ResponseString" type="xsd:string"/>
        </varResponseString>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.rbsas.co/1452097556047</pd:targetNamespace>
    <pd:activity name="Default Version">
        <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
        <pd:resourceType>ae.activities.null</pd:resourceType>
        <pd:x>195</pd:x>
        <pd:y>35</pd:y>
        <config/>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Validate Version">
        <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
        <pd:resourceType>ae.activities.null</pd:resourceType>
        <pd:x>263</pd:x>
        <pd:y>141</pd:y>
        <config/>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Call SetMerchantSystemTransaction">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>424</pd:x>
        <pd:y>141</pd:y>
        <config>
            <processName>/MerchanSystem.core/Implementation/SetMerchantSystemTransaction/SetMerchantSystemTransaction.process</processName>
            <processNameXPath>if (tib:trim($Start/pfx3:MerchantSystemTransactionRequest/pfx3:Version)="1.0.0") then 
"/MerchanSystem.core/Implementation/SetMerchantSystemTransaction/SetMerchantSystemTransaction.process" 
else "/MerchanSystem.core/Implementation/SetMerchantSystemTransaction/SetMerchantSystemTransaction.process"</processNameXPath>
        </config>
        <pd:inputBindings>
            <xsl:copy-of select="$Start/pfx3:MerchantSystemTransactionRequest"/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Process Response">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>581</pd:x>
        <pd:y>143</pd:y>
        <config>
            <variableName>varResponseString</variableName>
        </config>
        <pd:inputBindings>
            <ResponseString>
                <xsl:value-of select="$Call-SetMerchantSystemTransaction/ResponseString"/>
            </ResponseString>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Invalid Version Error">
        <pd:type>com.tibco.plugin.json.activities.JSONRenderActivity</pd:type>
        <pd:resourceType>ae.activities.JSONRenderActivity</pd:resourceType>
        <pd:x>267</pd:x>
        <pd:y>291</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <ActivityInputEditor ref="pfx4:MerchantSystemTransaction"/>
        </config>
        <pd:inputBindings>
            <ns:ActivityInputClass>
                <pfx4:MerchantSystemTransaction>
                    <pfx4:RequestDate>
                        <xsl:value-of select="tib:create-dateTime(&#xA;&#x9;tib:get-year-from-dateTime(current-dateTime()),&#xA;&#x9;tib:get-month-from-dateTime(current-dateTime()),&#xA;&#x9;tib:get-day-from-dateTime(current-dateTime()),&#xA;&#x9;tib:get-hours-from-dateTime(current-dateTime()),&#xA;&#x9;tib:get-minutes-from-dateTime(current-dateTime()),&#xA;&#x9;tib:get-seconds-from-dateTime(current-dateTime())&#xA;)"/>
                    </pfx4:RequestDate>
                    <pfx4:ErrorCount>
                        <xsl:value-of select="1"/>
                    </pfx4:ErrorCount>
                    <pfx4:Errors>
                        <pfx2:Error>
                            <pfx2:ErrorCode>
                                <xsl:value-of select="&quot;07&quot;"/>
                            </pfx2:ErrorCode>
                            <pfx2:ErrorMessage>
                                <xsl:value-of select="&quot;Versión inválida&quot;"/>
                            </pfx2:ErrorMessage>
                        </pfx2:Error>
                    </pfx4:Errors>
                </pfx4:MerchantSystemTransaction>
            </ns:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Error Response">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>577</pd:x>
        <pd:y>282</pd:y>
        <config>
            <variableName>varResponseString</variableName>
        </config>
        <pd:inputBindings>
            <ResponseString>
                <xsl:value-of select="$Invalid-Version-Error/ns:ActivityOutputClass/jsonString"/>
            </ResponseString>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Validate Version</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Default Version</pd:to>
        <pd:xpathDescription>Default</pd:xpathDescription>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>tib:trim($Start/pfx3:MerchantSystemTransactionRequest/pfx3:Version)=""</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Default Version</pd:from>
        <pd:to>Call SetMerchantSystemTransaction</pd:to>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Validate Version</pd:from>
        <pd:to>Call SetMerchantSystemTransaction</pd:to>
        <pd:xpathDescription>OK</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>tib:trim($Start/pfx3:MerchantSystemTransactionRequest/pfx3:Version)="1.0.0"</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Validate Version</pd:from>
        <pd:to>Invalid Version Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Invalid Version Error</pd:from>
        <pd:to>Error Response</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Call SetMerchantSystemTransaction</pd:from>
        <pd:to>Process Response</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Process Response</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Error Response</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>