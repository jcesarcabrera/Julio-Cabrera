                                    <xsl:value-of select="ns1:ProfileName"/>
                                </ns1:ProfileName>
                            </xsl:if>
                            <xsl:if test="ns1:TransactionType">
                                <ns1:TransactionType>
                                    <xsl:value-of select="ns1:TransactionType"/>
                                </ns1:TransactionType>
                            </xsl:if>
                            <xsl:if test="ns1:RechargeType">
