<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://www.rbsas.co/schemas/RBDAL/GeneralSchemas/Version 1.0.0/BusinessObjects/MerchantSystemTransaction.xsd" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns1="http://www.tibco.com/DaviviendaHCS/Hybrid_Card_Davivienda/Logs_Management/Schemas/AUDIT" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions">
    <pd:name>REST/BusinessProcesses/Version 1.0.0/SetMerchantSystemTransaction/BusinessClosing.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>61</pd:startX>
    <pd:startY>117</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>800</pd:endX>
    <pd:endY>120</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.rbsas.co/1463526630515</pd:targetNamespace>
    <pd:group name="Close Businesses">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>384</pd:x>
        <pd:y>34</pd:y>
        <pd:width>199</pd:width>
        <pd:height>154</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:over>$Get-Recharge-Networks-List/root/RechargeNetworksList/userId</pd:over>
            <pd:iterationElementSlot>rechargeNetworkId</pd:iterationElementSlot>
            <pd:indexSlot>i</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Close Business</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Close Business</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Close Business">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>484</pd:x>
            <pd:y>118</pd:y>
            <config>
                <processName>/GeneralProcesses/DataAccessProcesses/DalSetMerchantSystemTransaction.process</processName>
            </config>
            <pd:inputBindings>
                <root>
                    <TransactionId>
                        <xsl:value-of select="&quot;0000000000&quot;"/>
                    </TransactionId>
                    <TransactionAmount>
                        <xsl:value-of select="0"/>
                    </TransactionAmount>
                    <PosId>
                        <xsl:value-of select="&quot;0000000000&quot;"/>
                    </PosId>
                    <BusinessCode>
                        <xsl:value-of select="&quot;99&quot;"/>
                    </BusinessCode>
                    <UserId>
                        <xsl:value-of select="number($rechargeNetworkId/userId)"/>
                    </UserId>
                </root>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="End_Closing_Log">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>684</pd:x>
        <pd:y>121</pd:y>
        <config>
            <processName>/Audit_Management/Insert_Log.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns1:LOGS_SAVE>
                <ns1:process>
                    <xsl:value-of select="upper-case(BW:getCurrentProcessName($_processContext/ns:ProcessContext/ProcessId))"/>
                </ns1:process>
                <ns1:project>
                    <xsl:value-of select="&quot;RBInformationServices | BusinessClosing&quot;"/>
                </ns1:project>
                <ns1:datetime>
                    <xsl:value-of select="current-dateTime()"/>
                </ns1:datetime>
                <ns1:level>
                    <xsl:value-of select="&quot;INFO&quot;"/>
                </ns1:level>
                <ns1:message>
                    <xsl:value-of select="concat(&quot;Finaliza el proceso de cierre BusinessClosing: &quot;, $_processContext/ns:ProcessContext/ProcessId)"/>
                </ns1:message>
                <ns1:detail>
                    <xsl:value-of select="tib:render-xml($Close-Business/root/ns2:MerchantSystemTransaction)"/>
                </ns1:detail>
                <ns1:id_process>
                    <xsl:value-of select="$_processContext/ns:ProcessContext/ProcessId"/>
                </ns1:id_process>
            </ns1:LOGS_SAVE>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Get Recharge Networks List">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>182</pd:x>
        <pd:y>125</pd:y>
        <config>
            <processName>/GeneralProcesses/DataAccessProcesses/DalGetRechargeNetworks.process</processName>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Insert_Error">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>604</pd:x>
        <pd:y>243</pd:y>
        <config>
            <processName>/Audit_Management/Insert_Error.process</processName>
        </config>
        <pd:inputBindings>
            <ns1:Errors_Save xmlns:ns1="http://www.tibco.com/DaviviendaHCS/Hybrid_Card_Davivienda/Logs_Management/Schemas/Errors_Audit_Save">
                <ns1:error_message>
                    <xsl:value-of xmlns:xsl="http://www.w3.org/1999/XSL/Transform" select="$Get-Recharge-Networks-List/root/ns3:Error/ns3:ErrorMessage"/>
                </ns1:error_message>
                <ns1:id_process>
                    <xsl:value-of xmlns:xsl="http://www.w3.org/1999/XSL/Transform" select="$_processContext/ns:ProcessContext/ProcessId"/>
                </ns1:id_process>
            </ns1:Errors_Save>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log Error">
        <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
        <pd:resourceType>ae.activities.null</pd:resourceType>
        <pd:x>184</pd:x>
        <pd:y>291</pd:y>
        <config/>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Start_Closing_Log">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>294</pd:x>
        <pd:y>118</pd:y>
        <config>
            <processName>/Audit_Management/Insert_Log.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns1:LOGS_SAVE>
                <ns1:process>
                    <xsl:value-of select="upper-case(BW:getCurrentProcessName($_processContext/ns:ProcessContext/ProcessId))"/>
                </ns1:process>
                <ns1:project>
                    <xsl:value-of select="&quot;RBInformationServices | BusinessClosing&quot;"/>
                </ns1:project>
                <ns1:datetime>
                    <xsl:value-of select="current-dateTime()"/>
                </ns1:datetime>
                <ns1:level>
                    <xsl:value-of select="&quot;INFO&quot;"/>
                </ns1:level>
                <ns1:message>
                    <xsl:value-of select="concat(&quot;Inicia el proceso de cierre BusinessClosing: &quot;, $_processContext/ns:ProcessContext/ProcessId)"/>
                </ns1:message>
                <ns1:detail>
                    <xsl:value-of select="$Get-Recharge-Networks-List/root/RechargeNetworksList/userId[1]"/>
                </ns1:detail>
                <ns1:id_process>
                    <xsl:value-of select="$_processContext/ns:ProcessContext/ProcessId"/>
                </ns1:id_process>
            </ns1:LOGS_SAVE>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Get Recharge Networks List</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Get Recharge Networks List</pd:from>
        <pd:to>Log Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log Error</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Get Recharge Networks List</pd:from>
        <pd:to>Start_Closing_Log</pd:to>
        <pd:xpathDescription>OK</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Get-Recharge-Networks-List/root/Error = "false"</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Close Businesses</pd:from>
        <pd:to>End_Closing_Log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start_Closing_Log</pd:from>
        <pd:to>Close Businesses</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>End_Closing_Log</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>