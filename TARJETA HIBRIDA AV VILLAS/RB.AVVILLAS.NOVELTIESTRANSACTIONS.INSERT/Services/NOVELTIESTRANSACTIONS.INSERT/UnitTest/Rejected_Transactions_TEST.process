    <pd:activity name="SendMailAttachment">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>522</pd:x>
        <pd:y>35</pd:y>
        <config>
            <processName>/Commons/SendMailAttachment.process</processName>
        </config>
        <pd:inputBindings>
            <Mail>
                <mail_to>
                    <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/Mail/Report_Mail_To"/>
                </mail_to>
                <subject>
                    <xsl:value-of select="concat(&quot;Informe diario tarjeta híbrida Davivienda &quot;, tib:format-date(&quot;yyyy/MM/dd&quot;,tib:add-to-date(current-date(), 0, 0, -1)))"/>
                </subject>
                <fullfilename>
                    <xsl:value-of select="$Read_Acepted_Rejected_Transactions/Rejected_Files/Detail_Rejected"/>
                </fullfilename>
                <message>
                    <xsl:value-of select="concat(&quot;&lt;html>&lt;head>&lt;title>Informe diario tarjeta híbrida&quot;,&#xA; $_globalVariables/ns2:GlobalVariables/Validations_Response/Rejected_Transactions/BankName, &quot; &quot; ,&#xA;tib:format-date(&quot;yyyy/MM/dd&quot;, tib:add-to-date(current-date(), 0,0,-1)),&quot;&lt;/title>&lt;/head>&lt;body>&quot;,&#xA;&quot;&lt;h2>Informe diario tarjeta híbrida &quot;,&#xA; $_globalVariables/ns2:GlobalVariables/Validations_Response/Rejected_Transactions/BankName, &quot; &quot; ,&#xA;tib:format-date(&quot;yyyy/MM/dd&quot;, tib:add-to-date(current-date(), 0,0,-1)),&quot;&lt;/h2>&quot;,&#xA;&quot;&lt;p>&quot;,$GetLinesReport/Rows/line[2],&quot;&lt;/p>&quot;,&#xA;&quot;&lt;p>&quot;,$GetLinesReport/Rows/line[3],&quot;&lt;/p>&quot;,&#xA;&quot;&lt;p>&quot;,$GetLinesReport/Rows/line[4],&quot;&lt;/p>&quot;,&#xA;&quot;&lt;p>&quot;,$GetLinesReport/Rows/line[5],&quot;&lt;/p>&quot;,&#xA;&quot;&lt;p>&quot;,$GetLinesReport/Rows/line[6],&quot;&lt;/p>&quot;,&#xA;&quot;&lt;p>&quot;,$GetLinesReport/Rows/line[7],&quot;&lt;/p>&quot;,&#xA;&quot;&lt;p>&quot;,$GetLinesReport/Rows/line[8],&quot;&lt;/p>&quot;,&#xA;&quot;&lt;p>&quot;,$GetLinesReport/Rows/line[9],&quot;&lt;/p>&quot;,&#xA;&quot;&lt;p>&quot;,$GetLinesReport/Rows/line[10],&quot;&lt;/p>&quot;,&#xA;&quot;&lt;p>&quot;,$GetLinesReport/Rows/line[11],&quot;&lt;/p>&quot;,&#xA;&quot;&lt;p>&quot;,$GetLinesReport/Rows/line[12],&quot;&lt;/p>&quot;,&#xA;&quot;&lt;p>&quot;,$GetLinesReport/Rows/line[13],&quot;&lt;/p>&quot;,&#xA;&quot;&lt;p>&quot;,$GetLinesReport/Rows/line[14],&quot;&lt;/p>&quot;,&#xA;&quot;&lt;p>&quot;,$GetLinesReport/Rows/line[15],&quot;&lt;/p>&quot;,&#xA;&quot;&lt;p>&quot;,&quot;&lt;/p>&quot;,&#xA;&quot;'&lt;p>&#x9;&lt;h>Archivo adjunto detalles:&lt;/h>&lt;p>&quot;,&#xA;tib:substring-after-last($Read_Acepted_Rejected_Transactions/Rejected_Files/Detail_Rejected, &quot;\&quot;),&quot;&lt;p>&lt;/body>&lt;/html>&quot;)"/>
                </message>
                <id_proceso>
                    <xsl:value-of select="$_processContext/ns:ProcessContext/ProcessId"/>
                </id_proceso>
            </Mail>
        </pd:inputBindings>
