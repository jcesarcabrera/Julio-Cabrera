<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://www.tibco.com/pe/EngineTypes" xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx2="http://www.tibco.com/DaviviendaHCS/Hybrid_Card_Davivienda/Logs_Management/Schemas/Errors_Audit_Save" xmlns:pfx="http://www.tibco.com/DaviviendaHCS/Hybrid_Card_Davivienda/Logs_Management/Schemas/AUDIT">
    <pd:name>Novelties_Users_Transactions.Core/SendFilesMail/Send_AE_File/Send_AE_File.process</pd:name>
    <pd:startName>TimerAE</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="TimerAE">
        <pd:type>com.tibco.plugin.timer.TimerEventSource</pd:type>
        <pd:resourceType>ae.activities.timer</pd:resourceType>
        <pd:x>29</pd:x>
        <pd:y>35</pd:y>
        <config>
            <StartTime>%%Mail/Timer%%</StartTime>
            <Frequency>false</Frequency>
            <TimeInterval>1</TimeInterval>
            <FrequencyIndex>Day</FrequencyIndex>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>642</pd:endX>
    <pd:endY>35</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1472828463837</pd:targetNamespace>
    <pd:activity name="GET_AE_File">
        <pd:type>com.tibco.plugin.file.ListFilesActivity</pd:type>
        <pd:resourceType>ae.activities.ListFilesActivity</pd:resourceType>
        <pd:x>135</pd:x>
        <pd:y>35</pd:y>
        <config>
            <mode>files-and-directories</mode>
        </config>
        <pd:inputBindings>
            <ns1:ListFilesActivityConfig>
                <fileName>
                    <xsl:value-of select="concat($_globalVariables/ns:GlobalVariables/Validations_Request/Validations_File_Local_Path,&#xA;$_globalVariables/ns:GlobalVariables/Validations_Request/Validations_FileName_Prefix,&#xA;$_globalVariables/ns:GlobalVariables/Noveltys_Request/Bank_Code,&#xA;tib:format-date(&quot;yyyyMMdd&quot;, current-date()),&#xA;'*')"/>
                </fileName>
            </ns1:ListFilesActivityConfig>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="SendMailAttachment">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>371</pd:x>
        <pd:y>35</pd:y>
        <config>
            <processName>/Commons/SendMailAttachment.process</processName>
        </config>
        <pd:inputBindings>
            <Mail>
                <mail_to>
                    <xsl:value-of select="$_globalVariables/ns:GlobalVariables/Mail/AEFile_MailTo"/>
                </mail_to>
                <subject>
                    <xsl:value-of select="concat(&quot;Davivienda - Usos &quot;, tib:add-to-date(current-date(), 0, 0, -1))"/>
                </subject>
                <fullfilename>
                    <xsl:value-of select="$GET_AE_File/ns1:ListFilesActivityOutput/files/fileInfo[1]/fullName"/>
                </fullfilename>
                <message>
                    <xsl:value-of select="concat(&quot;Se envía adjunto Archivo con los usos enviados al banco correspondientes al día &quot;, tib:add-to-date(current-date(), 0, 0, -1))"/>
                </message>
                <id_proceso>
                    <xsl:value-of select="$_processContext/ns2:ProcessContext/ProcessId"/>
                </id_proceso>
            </Mail>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Start_Log">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>238</pd:x>
        <pd:y>35</pd:y>
        <config>
            <processName>/Audit_Management/Insert_Log.process</processName>
        </config>
        <pd:inputBindings>
            <pfx:LOGS_SAVE>
                <pfx:process>
                    <xsl:value-of select="upper-case(BW:getCurrentProcessName($_processContext/ns2:ProcessContext/ProcessId))"/>
                </pfx:process>
                <pfx:project>
                    <xsl:value-of select="concat(&quot;Tarjeta Hibirida Davivienda&quot;, &quot; | &quot;, $_processContext/ns2:ProcessContext/EngineName)"/>
                </pfx:project>
                <pfx:datetime>
                    <xsl:value-of select="current-dateTime()"/>
                </pfx:datetime>
                <pfx:level>
                    <xsl:value-of select="&quot;INFO&quot;"/>
                </pfx:level>
                <pfx:message>
                    <xsl:value-of select="concat(&quot;Comienza el proceso de envio del archivo de usos: &quot;,$GET_AE_File/ns1:ListFilesActivityOutput/files/fileInfo[1]/fullName)"/>
                </pfx:message>
                <xsl:if test="$GET_AE_File/ns1:ListFilesActivityOutput/files/fileInfo[1]/fileName">
                    <pfx:detail>
                        <xsl:value-of select="$GET_AE_File/ns1:ListFilesActivityOutput/files/fileInfo[1]/fileName"/>
                    </pfx:detail>
                </xsl:if>
                <pfx:id_process>
                    <xsl:value-of select="$_processContext/ns2:ProcessContext/ProcessId"/>
                </pfx:id_process>
            </pfx:LOGS_SAVE>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Insert_Error">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>641</pd:x>
        <pd:y>143</pd:y>
        <config>
            <processName>/Audit_Management/Insert_Error.process</processName>
        </config>
        <pd:inputBindings>
            <pfx2:Errors_Save>
                <pfx2:error_message>
                    <xsl:value-of select="$_error/ns2:ErrorReport/StackTrace"/>
                </pfx2:error_message>
                <pfx2:id_process>
                    <xsl:value-of select="$_processContext/ns2:ProcessContext/ProcessId"/>
                </pfx2:id_process>
            </pfx2:Errors_Save>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="End_log">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>503</pd:x>
        <pd:y>35</pd:y>
        <config>
            <processName>/Audit_Management/Insert_Log.process</processName>
        </config>
        <pd:inputBindings>
            <pfx:LOGS_SAVE>
                <pfx:process>
                    <xsl:value-of select="upper-case(BW:getCurrentProcessName($_processContext/ns2:ProcessContext/ProcessId))"/>
                </pfx:process>
                <pfx:project>
                    <xsl:value-of select="concat(&quot;Tarjeta Hibirida Davivienda&quot;, &quot; | &quot;, $_processContext/ns2:ProcessContext/EngineName)"/>
                </pfx:project>
                <pfx:datetime>
                    <xsl:value-of select="current-dateTime()"/>
                </pfx:datetime>
                <pfx:level>
                    <xsl:value-of select="&quot;INFO&quot;"/>
                </pfx:level>
                <pfx:message>
                    <xsl:value-of select="concat(&quot;Finaliza el proceso de envio del archivo de usos: &quot;,$GET_AE_File/ns1:ListFilesActivityOutput/files/fileInfo[1]/fullName)"/>
                </pfx:message>
                <xsl:if test="$GET_AE_File/ns1:ListFilesActivityOutput/files/fileInfo[1]/fileName">
                    <pfx:detail>
                        <xsl:value-of select="$GET_AE_File/ns1:ListFilesActivityOutput/files/fileInfo[1]/fileName"/>
                    </pfx:detail>
                </xsl:if>
                <pfx:id_process>
                    <xsl:value-of select="$_processContext/ns2:ProcessContext/ProcessId"/>
                </pfx:id_process>
            </pfx:LOGS_SAVE>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>510</pd:x>
        <pd:y>141</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>TimerAE</pd:from>
        <pd:to>GET_AE_File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GET_AE_File</pd:from>
        <pd:to>Start_Log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start_Log</pd:from>
        <pd:to>SendMailAttachment</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SendMailAttachment</pd:from>
        <pd:to>End_log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>End_log</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Insert_Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Insert_Error</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>