                <statement>INSERT INTO TransactionsResponse (TransactionSequenceNumber, DocumentID, BankCode, UsageDate, AmountUsed, FCSCardNumber, BusStopNm, BusStopID, ResponseCode, ARFileName) VALUES (?,?,?,?,?,?,?,?,?,?)</statement>
                <Prepared_Param_DataType>
                    <parameter>
