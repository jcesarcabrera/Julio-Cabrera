<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="http://www.tibco.com/CommonLE2/namespace/Private/InsertLogRequest.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions" xmlns:ns1="http://www.tibco.com/DaviviendaHCS/Hybrid_Card_Davivienda/Logs_Management/Schemas/AUDIT" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:ns3="http://www.tibco.com/DaviviendaHCS/Hybrid_Card_Davivienda/Logs_Management/Schemas/Errors_Audit_Save" xmlns:ns0="www.tibco.com/plugin/Sleep" xmlns:pfx5="http://www.recaudo.bogota.com/schemas/ws/GetFilesSFTP" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx4="http://www.recaudo.bogota.com/schemas/ws/Schema" xmlns:pfx3="http://www.tibco.com/CommonLE2/namespace/Public/Common/RequestHeader.xsd" xmlns:pfx2="http://www.tibco.com/CommonLE2/namespace/Public/Logging/LogRequest.xsd" xmlns:pfx="http://www.tibco.com/pe/DeployedVarsType" xmlns:pfx6="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema">
    <xsd:import namespace="http://www.recaudo.bogota.com/schemas/ws/GetFilesSFTP"/>
    <xsd:import namespace="http://www.recaudo.bogota.com/schemas/ws/GetFilesSFTP" schemaLocation="/Interfaces/Schemas/GetFilesSFTP.xsd"/>
    <pd:name>GetNoveltysFromBank/SFTPGetFiles.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>49</pd:startX>
    <pd:startY>165</pd:startY>
    <pd:returnBindings>
        <root>
            <filename>
                <xsl:value-of select="tib:substring-after-last($SFTP-Get/SFTPGetOutputFile/FileTransferred[1]/Name, '/')"/>
            </filename>
            <fullfilename>
                <xsl:value-of select="concat($_globalVariables/pfx:GlobalVariables/SendNoveltys/LocalDirectories/PathFileEE,tib:substring-after-last($SFTP-Get/SFTPGetOutputFile/FileTransferred[1]/Name, '/'))"/>
            </fullfilename>
        </root>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="filename" type="xsd:string"/>
                    <xsd:element name="fullfilename" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>820</pd:endX>
    <pd:endY>163</pd:endY>
    <pd:errorSchemas>
        <Error>
            <xsd:element name="ErrorDescription" type="xsd:string"/>
        </Error>
    </pd:errorSchemas>
    <pd:processVariables>
        <Retries>
            <xsd:element name="Retry" type="xsd:string"/>
        </Retries>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1433968084241</pd:targetNamespace>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>230</pd:x>
        <pd:y>2</pd:y>
        <pd:width>331</pd:width>
        <pd:height>309</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>repeat</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>index</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:repeatCondition>$Retries/Retry &gt;= $_globalVariables/pfx:GlobalVariables/SendNoveltys/SFTP/Retries or $Retries/Retry='OK'</pd:repeatCondition>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>SFTP Dir</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>SetRetry</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Insert_Error</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Catch</pd:from>
            <pd:to>Sleep</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>SFTP Get</pd:from>
            <pd:to>SetRetry</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Sleep</pd:from>
            <pd:to>SetRetryError</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>SFTP Dir</pd:from>
            <pd:to>SFTP Get</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>otherwise</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>SetRetryError</pd:from>
            <pd:to>Insert_Error</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>SFTP Dir</pd:from>
            <pd:to>FileNotFound</pd:to>
            <pd:xpathDescription>FileNotFound</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-65536</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>$SFTP-Dir/Output/ItemCount=0</pd:xpath>
        </pd:transition>
        <pd:activity name="SFTP Get">
            <pd:type>com.tibco.plugin.sp.SFTPGetActivity</pd:type>
            <pd:resourceType>ae.activities.SFTPGetActivityUI</pd:resourceType>
            <pd:x>399</pd:x>
            <pd:y>58</pd:y>
            <config>
                <Timeout>120000</Timeout>
                <isBinary>false</isBinary>
                <Overwrite>true</Overwrite>
                <useProcessData>false</useProcessData>
                <logout>true</logout>
                <SharedUserData>/GetNoveltysFromBank/SFTPInterface.sharedsftp</SharedUserData>
            </config>
            <pd:inputBindings>
                <SFTPGetInputDataFile>
                    <RemoteFileName>
                        <xsl:value-of select="concat($_globalVariables/pfx:GlobalVariables/SendNoveltys/SFTP/RemotePathSendNoveltys,$SFTP-Dir/Output/DirectoryItems[1])"/>
                    </RemoteFileName>
                    <LocalFileName>
                        <xsl:value-of select="$_globalVariables/pfx:GlobalVariables/SendNoveltys/LocalDirectories/PathFileEE"/>
                    </LocalFileName>
                </SFTPGetInputDataFile>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Catch">
            <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
            <pd:resourceType>ae.activities.catch</pd:resourceType>
            <pd:x>264</pd:x>
            <pd:y>257</pd:y>
            <pd:handler>true</pd:handler>
            <config>
                <catchAll>true</catchAll>
            </config>
            <pd:inputBindings/>
        </pd:activity>
        <pd:activity name="SetRetry">
            <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
            <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
            <pd:x>513</pd:x>
            <pd:y>58</pd:y>
            <config>
                <variableName>Retries</variableName>
            </config>
            <pd:inputBindings>
                <Retry>
                    <xsl:value-of select="'OK'"/>
                </Retry>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Sleep">
            <pd:type>com.tibco.plugin.timer.SleepActivity</pd:type>
            <pd:resourceType>ae.activities.sleep</pd:resourceType>
            <pd:x>330</pd:x>
            <pd:y>257</pd:y>
            <config/>
            <pd:inputBindings>
                <ns0:SleepInputSchema>
                    <IntervalInMillisec>
                        <xsl:value-of select="$_globalVariables/pfx:GlobalVariables/SendNoveltys/SFTP/Sleep"/>
                    </IntervalInMillisec>
                </ns0:SleepInputSchema>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="SetRetryError">
            <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
            <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
            <pd:x>416</pd:x>
            <pd:y>256</pd:y>
            <config>
                <variableName>Retries</variableName>
            </config>
            <pd:inputBindings>
                <Retry>
                    <xsl:value-of select="$Retries/Retry+1"/>
                </Retry>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="SFTP Dir">
            <pd:type>com.tibco.plugin.sp.SFTPDirActivity</pd:type>
            <pd:resourceType>ae.activities.SFTPDirActivityUI</pd:resourceType>
            <pd:x>282</pd:x>
            <pd:y>58</pd:y>
            <config>
                <NLST>true</NLST>
                <SharedUserData>/GetNoveltysFromBank/SFTPInterface.sharedsftp</SharedUserData>
            </config>
            <pd:inputBindings>
                <Input>
                    <Directory>
                        <xsl:value-of select="concat($_globalVariables/pfx:GlobalVariables/SendNoveltys/SFTP/RemotePathSendNoveltys,'EE',$_globalVariables/pfx:GlobalVariables/SendNoveltys/BankInfo/FileCode,tib:format-dateTime('yyyyMMdd',tib:add-to-dateTime(current-dateTime(), 0, 0, 0 ,0, 0,0)),'*.pgp')"/>
                    </Directory>
                </Input>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Insert_Error">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>511</pd:x>
            <pd:y>257</pd:y>
            <config>
                <processName>/Audit_Management/Insert_Error.process</processName>
                <spawn>true</spawn>
            </config>
            <pd:inputBindings>
                <ns3:Errors_Save>
                    <ns3:error_message>
                        <xsl:value-of select="$_error/ns:ErrorReport/StackTrace"/>
                    </ns3:error_message>
                    <ns3:id_process>
                        <xsl:value-of select="$_processContext/ns:ProcessContext/ProcessId"/>
                    </ns3:id_process>
                </ns3:Errors_Save>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="FileNotFound">
            <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
            <pd:resourceType>ae.activities.throw</pd:resourceType>
            <pd:x>398</pd:x>
            <pd:y>148</pd:y>
            <config>
                <faultName>Error</faultName>
            </config>
            <pd:inputBindings>
                <pfx6:ActivityInput>
                    <data>
                        <ErrorDescription>
                            <xsl:value-of select="'Archivo de novedades no encontrado en SFTP'"/>
                        </ErrorDescription>
                    </data>
                </pfx6:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Initialize">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>154</pd:x>
        <pd:y>165</pd:y>
        <config>
            <variableName>Retries</variableName>
        </config>
        <pd:inputBindings>
            <Retry>
                <xsl:value-of select="1"/>
            </Retry>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Generate Error">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>752</pd:x>
        <pd:y>36</pd:y>
        <config>
            <faultName/>
        </config>
        <pd:inputBindings>
            <pfx6:ActivityInput>
                <message>
                    <xsl:value-of select="'No se pudo realizar obtencion de archivo de envio de novedades intentos agotados'"/>
                </message>
            </pfx6:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Insert_Log">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>673</pd:x>
        <pd:y>165</pd:y>
        <config>
            <processName>/Audit_Management/Insert_Log.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns1:LOGS_SAVE>
                <ns1:process>
                    <xsl:value-of select="BW:getCurrentProcessName($_processContext/ns:ProcessContext/ProcessId)"/>
                </ns1:process>
                <ns1:project>
                    <xsl:value-of select="concat($_globalVariables/pfx:GlobalVariables/SendNoveltys/ProjectName, &quot; | &quot;, $_processContext/ns:ProcessContext/EngineName)"/>
                </ns1:project>
                <ns1:datetime>
                    <xsl:value-of select="current-dateTime()"/>
                </ns1:datetime>
                <ns1:level>
                    <xsl:value-of select="'Info'"/>
                </ns1:level>
                <ns1:message>
                    <xsl:value-of select="'Obtencion de archivos del SFTP envio de novedades'"/>
                </ns1:message>
                <ns1:detail>
                    <xsl:value-of select="tib:render-xml($SFTP-Get/SFTPGetOutputFile)"/>
                </ns1:detail>
                <ns1:id_process>
                    <xsl:value-of select="$_processContext/ns:ProcessContext/ProcessId"/>
                </ns1:id_process>
            </ns1:LOGS_SAVE>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>Insert_Log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Initialize</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Initialize</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>Generate Error</pd:to>
        <pd:xpathDescription>Error</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Retries/Retry!='OK'</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Insert_Log</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>