<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions">
    <pd:name>GetNoveltysFromBank/ConvertFileERToHex.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="fullfilename" type="xsd:string"/>
    </pd:startType>
    <pd:startX>31</pd:startX>
    <pd:startY>56</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endType/>
    <pd:endX>396</pd:endX>
    <pd:endY>56</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1473957597793</pd:targetNamespace>
    <pd:activity name="ParseFileER">
        <pd:type>com.tibco.plugin.parse.ParseActivity</pd:type>
        <pd:resourceType>ae.activities.ParseActivity</pd:resourceType>
        <pd:x>117</pd:x>
        <pd:y>56</pd:y>
        <config>
            <InputType>File</InputType>
            <Encoding>ASCII</Encoding>
            <ParseSharedConfig>/GetNoveltysFromBank/DataFormatBankResult64.sharedparse</ParseSharedConfig>
        </config>
        <pd:inputBindings>
            <Input>
                <fileName>
                    <xsl:value-of select="$Start/fullfilename"/>
                </fileName>
                <noOfRecords>
                    <xsl:value-of select="-1"/>
                </noOfRecords>
            </Input>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="RenderFileER">
        <pd:type>com.tibco.plugin.parse.RenderActivity</pd:type>
        <pd:resourceType>ae.activities.RenderActivity</pd:resourceType>
        <pd:x>215</pd:x>
        <pd:y>56</pd:y>
        <config>
            <ParseSharedConfig>/GetNoveltysFromBank/DataFormatBankResultNot64.sharedparse</ParseSharedConfig>
        </config>
        <pd:inputBindings>
            <Rows>
                <xsl:for-each select="$ParseFileER/Output/Rows/root">
                    <root>
                        <document_id>
                            <xsl:value-of select="document_id"/>
                        </document_id>
                        <card_number>
                            <xsl:value-of select="tib:base64-to-hex(card_number)"/>
                        </card_number>
                        <names>
                            <xsl:value-of select="names"/>
                        </names>
                        <last_name>
                            <xsl:value-of select="last_name"/>
                        </last_name>
                        <second_last_name>
                            <xsl:value-of select="second_last_name"/>
                        </second_last_name>
                        <address>
                            <xsl:value-of select="address"/>
                        </address>
                        <status>
                            <xsl:value-of select="status"/>
                        </status>
                    </root>
                </xsl:for-each>
            </Rows>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="WriteFileER">
        <pd:type>com.tibco.plugin.file.FileWriteActivity</pd:type>
        <pd:resourceType>ae.activities.FileWriteActivity</pd:resourceType>
        <pd:x>309</pd:x>
        <pd:y>56</pd:y>
        <config>
            <encoding>text</encoding>
            <compressFile>None</compressFile>
        </config>
        <pd:inputBindings>
            <ns:WriteActivityInputTextClass>
                <fileName>
                    <xsl:value-of select="concat($Start/fullfilename,'_HEX')"/>
                </fileName>
                <textContent>
                    <xsl:value-of select="$RenderFileER/Text"/>
                </textContent>
            </ns:WriteActivityInputTextClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>ParseFileER</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>ParseFileER</pd:from>
        <pd:to>RenderFileER</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>RenderFileER</pd:from>
        <pd:to>WriteFileER</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>WriteFileER</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>