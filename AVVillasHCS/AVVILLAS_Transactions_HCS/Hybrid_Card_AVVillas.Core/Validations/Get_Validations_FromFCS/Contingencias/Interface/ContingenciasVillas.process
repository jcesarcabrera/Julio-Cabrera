<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns1="http://www.tibco.com/DaviviendaHCS/Hybrid_Card_Davivienda/Logs_Management/Schemas/AUDIT" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:ns3="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx2="http://www.tibco.com/schemas/Davivienda_Transactions_HCS/Hybrid_Card_Davivienda.Core/Validations/Get_Validations_FromFCS/Contingencias/Schema.xsd2" xmlns:ns12="http://www.tibco.com/DaviviendaHCS/Hybrid_Card_Davivienda/Logs_Management/Schemas/Errors_Audit_Save" xmlns:ns13="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:pfx="http://www.tibco.com/schemas/Davivienda_Transactions_HCS/Hybrid_Card_Davivienda.Core/Validations/Get_Validations_FromFCS/Contingencias/Schema.xsd">
    <xsd:import namespace="http://www.tibco.com/schemas/Davivienda_Transactions_HCS/Hybrid_Card_Davivienda.Core/Validations/Get_Validations_FromFCS/Contingencias/Schema.xsd" schemaLocation="/Hybrid_Card_AVVillas.Core/Validations/Get_Validations_FromFCS/Contingencias/ContingenciasIN.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/Davivienda_Transactions_HCS/Hybrid_Card_Davivienda.Core/Validations/Get_Validations_FromFCS/Contingencias/Schema.xsd2" schemaLocation="/Hybrid_Card_AVVillas.Core/Validations/Get_Validations_FromFCS/Contingencias/ContingenciasOUT.xsd"/>
    <pd:name>Hybrid_Card_AVVillas.Core/Validations/Get_Validations_FromFCS/Contingencias/Interface/ContingenciasVillas.process</pd:name>
    <pd:startName>Start_Contingencia</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings>
        <xsl:choose>
            <xsl:when test="empty($_error/ns:ErrorReport)">
                <pfx2:Contingencias>
                    <pfx2:Mensaje>
                        <xsl:value-of select="concat(&quot;Se ha generado correctamente el archivo de contingencias para el banco AV Villas en la siguiente ruta: &quot;, $Contingencias/ArchivoContingencia)"/>
                    </pfx2:Mensaje>
                    <pfx2:FechaConsulta>
                        <xsl:value-of select="current-dateTime()"/>
                    </pfx2:FechaConsulta>
                    <pfx2:Estado>
                        <xsl:value-of select="concat(&quot;Correcto,  | &quot;, $_processContext/ns:ProcessContext/ProcessId)"/>
                    </pfx2:Estado>
                </pfx2:Contingencias>
            </xsl:when>
            <xsl:otherwise>
                <pfx2:Contingencias>
                    <pfx2:Mensaje>
                        <xsl:value-of select="concat(&quot;Ha ocurrido un error al generar el archivo de contingencia para el banco AV Villas: &quot;, $_error/ns:ErrorReport/Msg)"/>
                    </pfx2:Mensaje>
                    <pfx2:FechaConsulta>
                        <xsl:value-of select="current-dateTime()"/>
                    </pfx2:FechaConsulta>
                    <pfx2:Estado>
                        <xsl:value-of select="concat(&quot;Error: &quot;, $_error/ns:ErrorReport/StackTrace)"/>
                    </pfx2:Estado>
                </pfx2:Contingencias>
            </xsl:otherwise>
        </xsl:choose>
    </pd:returnBindings>
    <pd:starter name="Start_Contingencia">
        <pd:type>com.tibco.plugin.jms.JMSQueueEventSource</pd:type>
        <pd:resourceType>ae.activities.JMSQueueEventSource</pd:resourceType>
        <pd:x>60</pd:x>
        <pd:y>35</pd:y>
        <config>
            <PermittedMessageType>Text</PermittedMessageType>
            <SessionAttributes>
                <transacted>false</transacted>
                <acknowledgeMode>1</acknowledgeMode>
                <maxSessions>1</maxSessions>
                <destination>%%RecaudoBogota/Contingencias/QueueContingencia%%</destination>
            </SessionAttributes>
            <ConfigurableHeaders>
                <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                <JMSExpiration>0</JMSExpiration>
                <JMSPriority>4</JMSPriority>
            </ConfigurableHeaders>
            <ConnectionReference>/Hybrid_Card_AVVillas.Core/Validations/Get_Validations_FromFCS/EMS_Connection.sharedjmscon</ConnectionReference>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx2:Contingencias"/>
    <pd:endX>634</pd:endX>
    <pd:endY>35</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1473345457915</pd:targetNamespace>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>520</pd:x>
        <pd:y>146</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Contingencias">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>399</pd:x>
        <pd:y>35</pd:y>
        <config>
            <processName>/Hybrid_Card_AVVillas.Core/Validations/Get_Validations_FromFCS/Contingencias/Contingencias.process</processName>
        </config>
        <pd:inputBindings>
            <pfx:Contingencias>
                <pfx:FechaConsulta>
                    <xsl:value-of select="$Start_Contingencia/ns2:ActivityOutput/Body"/>
                </pfx:FechaConsulta>
            </pfx:Contingencias>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="End_log">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>513</pd:x>
        <pd:y>35</pd:y>
        <config>
            <spawn>true</spawn>
            <processName>/Audit_Management/Insert_Log.process</processName>
        </config>
        <pd:inputBindings>
            <ns1:LOGS_SAVE>
                <ns1:process>
                    <xsl:value-of select="BW:getCurrentProcessName($_processContext/ns:ProcessContext/ProcessId)"/>
                </ns1:process>
                <ns1:project>
                    <xsl:value-of select="concat(&quot;Tarjeta Híbrida AV Villas&quot;, &quot; | &quot;, $_processContext/ns:ProcessContext/EngineName)"/>
                </ns1:project>
                <ns1:datetime>
                    <xsl:value-of select="current-dateTime()"/>
                </ns1:datetime>
                <ns1:level>
                    <xsl:value-of select="&quot;INFO&quot;"/>
                </ns1:level>
                <ns1:message>
                    <xsl:value-of select="concat(&quot;Tarjeta Híbrida AV Villas&quot;, &quot; | &quot;,&quot;Finaliza la operación de extracción y envío de usos desde el FCS al Banco por Contingencia ID Audit: &quot;,$_processContext/ns:ProcessContext/ProcessId)"/>
                </ns1:message>
                <ns1:id_process>
                    <xsl:value-of select="$_processContext/ns:ProcessContext/ProcessId"/>
                </ns1:id_process>
            </ns1:LOGS_SAVE>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Insert_Error">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>635</pd:x>
        <pd:y>146</pd:y>
        <config>
            <spawn>true</spawn>
            <processName>/Audit_Management/Insert_Error.process</processName>
        </config>
        <pd:inputBindings>
            <ns12:Errors_Save>
                <ns12:error_message>
                    <xsl:value-of select="$_error/ns:ErrorReport/StackTrace"/>
                </ns12:error_message>
                <ns12:id_process>
                    <xsl:value-of select="$_processContext/ns:ProcessContext/ProcessId"/>
                </ns12:id_process>
            </ns12:Errors_Save>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="List-Files">
        <pd:type>com.tibco.plugin.file.ListFilesActivity</pd:type>
        <pd:resourceType>ae.activities.ListFilesActivity</pd:resourceType>
        <pd:x>271</pd:x>
        <pd:y>35</pd:y>
        <config>
            <mode>files-and-directories</mode>
        </config>
        <pd:inputBindings>
            <ns13:ListFilesActivityConfig>
                <fileName>
                    <xsl:value-of select="concat(&#xA;$_globalVariables/ns3:GlobalVariables/RecaudoBogota/Contingencias/Local_Path,&#xA;$_globalVariables/ns3:GlobalVariables/RecaudoBogota/Validations_Request/Validations_FileName_Prefix,&#xA;$_globalVariables/ns3:GlobalVariables/RecaudoBogota/Bank_Code,&#xA;$Start_Contingencia/ns2:ActivityOutput/Body,&#xA;'*')"/>
                </fileName>
            </ns13:ListFilesActivityConfig>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Remove File">
        <pd:type>com.tibco.plugin.file.FileRemoveActivity</pd:type>
        <pd:resourceType>ae.activities.FileRemoveActivity</pd:resourceType>
        <pd:x>274</pd:x>
        <pd:y>145</pd:y>
        <config/>
        <pd:inputBindings>
            <ns13:RemoveActivityInputClass>
                <fileName>
                    <xsl:value-of select="$List-Files/ns13:ListFilesActivityOutput/files/fileInfo[1]/fullName"/>
                </fileName>
            </ns13:RemoveActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Start_Log">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>165</pd:x>
        <pd:y>35</pd:y>
        <config>
            <spawn>true</spawn>
            <processName>/Audit_Management/Insert_Log.process</processName>
        </config>
        <pd:inputBindings>
            <ns1:LOGS_SAVE>
                <ns1:process>
                    <xsl:value-of select="BW:getCurrentProcessName($_processContext/ns:ProcessContext/ProcessId)"/>
                </ns1:process>
                <ns1:project>
                    <xsl:value-of select="concat(&quot;Tarjeta Híbrida AV Villas&quot;, &quot; | &quot;, $_processContext/ns:ProcessContext/EngineName)"/>
                </ns1:project>
                <ns1:datetime>
                    <xsl:value-of select="current-dateTime()"/>
                </ns1:datetime>
                <ns1:level>
                    <xsl:value-of select="&quot;INFO&quot;"/>
                </ns1:level>
                <ns1:message>
                    <xsl:value-of select="concat(&quot;Tarjeta Híbrida AV Villas&quot;, &quot; | &quot;,&quot;Inicia la operación de extracción y envío de usos desde el FCS al Banco por Contingencia ID Audit: &quot;,$_processContext/ns:ProcessContext/ProcessId)"/>
                </ns1:message>
                <ns1:id_process>
                    <xsl:value-of select="$_processContext/ns:ProcessContext/ProcessId"/>
                </ns1:id_process>
            </ns1:LOGS_SAVE>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Insert_Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Contingencias</pd:from>
        <pd:to>End_log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>End_log</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Insert_Error</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start_Contingencia</pd:from>
        <pd:to>Start_Log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start_Log</pd:from>
        <pd:to>List-Files</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>List-Files</pd:from>
        <pd:to>Remove File</pd:to>
        <pd:xpathDescription>Exist</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>count($List-Files/ns13:ListFilesActivityOutput/files/fileInfo)&gt;0</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Remove File</pd:from>
        <pd:to>Contingencias</pd:to>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>List-Files</pd:from>
        <pd:to>Contingencias</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>